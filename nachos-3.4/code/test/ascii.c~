/* ascii.c 
 *    Test program to ascii table.
 *
 *    Intention is to stress virtual memory system.
 */

#include "syscall.h"

int
main()
{
    int i;
	PrintString("\n--Chuong trinh in bang ma ascii--\n");
    for (i = 0; i < 255; i++){
		PrintString("Ki tu thu i: ");		
		PrintInt(i);
		PrintString(": ");
		PrintChar((char)i);
		PrintString("\n");
	}

	return 0;
}
